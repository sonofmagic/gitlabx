{
  "name": "@gitlabx/cli",
  "type": "module",
  "version": "0.0.0",
  "description": "tsdown bundler template",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/gitlabx.git",
    "directory": "apps/cli"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/gitlabx/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "gitlabx": "./dist/cli.cjs",
    "gbx": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "cli": "tsx src/index.ts",
    "dev": "tsdown -w",
    "build": "tsdown",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {},
  "dependencies": {
    "@gitbeaker/rest": "^43.8.0",
    "@inquirer/prompts": "^8.2.0",
    "c12": "^3.3.3",
    "commander": "^14.0.0",
    "picocolors": "^1.1.0"
  }
}
